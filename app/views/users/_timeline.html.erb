<div class="portlet light portlet-fit", id="timeline-container">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-commenting-o font-green"></i>
            <span class="caption-subject bold font-green uppercase"> Garis Masa</span>
        </div>
    </div>
    <div class="portlet-body">
        <div class="timeline">
          <div class="scroller" style="height: 600px;" data-always-visible="1" data-rail-visible="0">
            <!-- TIMELINE ITEM -->
              <%- @statuses.each do |status| %>           
               <%= render partial: 'statuses/timeline_template', locals: { status: status }%>
              <% end -%>
          </div>
        </div>
    </div>
</div>
